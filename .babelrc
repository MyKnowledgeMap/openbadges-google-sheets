{
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-html-import-to-string"
  ],
  "env": {
    "test": {
      "presets": [["env", { "modules": "commonjs" }]],
      "sourceMaps": "both"
    },
    "development": {
      "presets": [["env", { "modules": false }]],
      "plugins": ["transform-remove-export"],
      "ignore": ["**/*.spec.js", "**/*.mock.js"]
    }
  }
}
